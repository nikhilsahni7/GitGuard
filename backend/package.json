{
  "name": "backend",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --target=node --outdir=dist",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "bun seed.ts",
    "permit:setup": "bun setup-permit.ts",
    "permit:verify": "bun verify-permit.ts",
    "permit:pdp": "docker run -d --name permit-pdp -p 7766:7000 -e PDP_API_KEY=${PERMIT_API_KEY} -e PDP_DEBUG=true permitio/pdp-v2:latest",
    "permit:test": "bun test-permit.ts",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
"@prisma/client": "^5.0.0",

    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.8",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "expo-server-sdk": "3.7.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "permitio": "^2.7.4",
    "prisma": "^5.0.0",
    "resend": "^4.5.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  }
}
